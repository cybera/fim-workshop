externalGroupProviders:
{% for provider in voot.externalGroupProviders %}
  - {
      type: "{{ provider.type }}",
      url: "{{ provider.url }}",
      credentials: {
        username: "{{ provider.credentials.username }}",
        secret: "{{ provider.credentials.secret }}"
      },
      schacHomeOrganization: "{{ provider.schacHomeOrganization }}",
      name: "{{ provider.name }}",
      timeoutMillis: {{ provider.timeoutMillis }}
     }
{% endfor %}
